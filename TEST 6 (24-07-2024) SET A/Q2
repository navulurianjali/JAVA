public class Car {
    private String model;
    private int year;

    public Car(String model, int year) {
        this.model = model;
        this.year = year;
    }

    public void display() {
        System.out.println("model: " + model + ", year: " + year);
    }

    public static void main(String[] args) {
        Car car = new Car("Toyota", 2020);
        car.display();
    }
}
