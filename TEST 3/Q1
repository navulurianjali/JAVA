public class Complex {
    private double real;
    private double imag;
public Complex(double real, double imag) {
        this.real = real;
        this.imag = imag;
    }
public Complex add(Complex other) {
    return new Complex(this.real + other.real, this.imag + other.imag);
    }
public Complex add(double real, double imag) {
        return new Complex(this.real + real, this.imag + imag);
    }
    @Override
    public String toString() {
        return String.format("%f + %fi", real, imag);
    }
public static void main(String[] args) {
    Complex c1 = new Complex(3, 4);
Complex c2 = new Complex(2, 5);
        Complex c3 = c1.add(c2);
System.out.println("add of 2 numbers = " + c3);
        Complex c4 = c1.add(1, 2);
        System.out.println("sum = " + c4);
    }
}
