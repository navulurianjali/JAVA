public class MethodOverloadingDemo {
     public void display(int num, String message) {
        System.out.println("You passed an integer and a string: " + num + ", " + message);
    }

    public void display(double num, String message) {
        System.out.println("You passed a double and a string: " + num + ", " + message);
}
    public static void main(String[] args) {
        MethodOverloadingDemo demo = new MethodOverloadingDemo();

        demo.display(07, "welcome");

        demo.display(123.0908, "hyderabad");
    }
}
